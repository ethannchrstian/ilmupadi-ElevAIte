generator client {
  provider = "prisma-client-js"
  // output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Analysis {
  id         Int      @id @default(autoincrement())
  imageName  String
  imagePath  String
  prediction String
  createdAt  DateTime @default(now())
}
